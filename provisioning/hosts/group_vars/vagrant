---
user_root: vagrant

path_front: /var/www/front-openorchestra

hosts_localhost: 127.0.0.1 localhost openorchestra

hosts_site: 127.0.1.1 admin.openorchestra.dev_stable openorchestra.test front.openorchestra.dev_stable demo.openorchestra.dev_stable media.openorchestra.dev_stable

apache_main_ports:
  - 80

back_ports:
  - 80

apache_conf:
  open-orchestra.conf:
    port: 80
    serverName: admin.openorchestra.dev_stable
    docRoot: /var/www/openorchestra/web
    errorLog: admin.openorchestraError.log
    accessLog: admin.openorchestraAccess.log
  demo-orchestra.conf:
    port: 80
    serverName: demo.openorchestra.dev_stable
    docRoot: /var/www/front-openorchestra/web
    errorLog: demo.openorchestraError.log
    accessLog: demo.openorchestraAccess.log
    siteId:
  media-orchestra.conf:
    port: 80
    serverName: media.openorchestra.dev_stable
    docRoot: /var/www/media-openorchestra/web
    errorLog: media.openorchestraError.log
    accessLog: media.openorchestraAccess.log
  open-orchestra-test.conf:
    port: 80
    serverName: openorchestra.test
    docRoot: /var/www/openorchestra/web
    errorLog: openorchestra-testError.log
    accessLog: openorchestra-testAccess.log

varnish_listen_port: 6081

backend_conf:
  - { name: openorchestra, port: 80, host: admin.openorchestra.dev_stable, admin: true }
  - { name: frontopenorchestra, port: 80, host: front.openorchestra.dev_stable }
  - { name: demoopenorchestra, port: 80, host: demo.openorchestra.dev_stable }
  - { name: openorchestratest, port: 80, host: openorchestra.test }
  - { name: mediaopenorchestra, port: 80, host: media.openorchestra.dev_stable }

selenium_current_directory: /home/vagrant

mail_to: cron@open-orchestra.com

sudoers:
  - vagrant
