{% if resultset.spellcheck.correctlySpelled is empty%}
    <ul>
    {% for suggestion in resultset.spellcheck %}
        {{ 'Voulez vous dire '}}<a href="{{ path('php_orchestra_cms_node', {'nodeId': nodeId, 'Search': suggestion.word }) }}">{{ suggestion.word }}</a>
    {% endfor %}
    </ul>
{% endif %}