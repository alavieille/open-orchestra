{% block orchestra_customField_widget %}
<fieldset>
    <legend>Field type : {{ form.vars.value.type }}</legend>
{#{ dump(form.vars.value)}#}

{#% for key, value in form.vars %}
    {{ key }} <br />
{% endfor %#}

    <table>
        <tr><td>Nom du champ</td><td><input type="text" value="" /></td></tr>
        <tr><td>Identifiant</td><td><input type="text" value="{{ form.vars.value.fieldId }}" /></td></tr>
        <tr><td>Indexable</td><td><input type="checkbox" /></td></tr>
    </table>
</fieldset>
{% endblock %}