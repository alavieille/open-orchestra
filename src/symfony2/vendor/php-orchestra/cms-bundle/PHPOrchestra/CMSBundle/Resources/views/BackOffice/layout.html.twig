<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <title>{% block title %}PHPOrchestra{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/phporchestracms/css/backoffice.css') }}">
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </head>
    <body class="fixed-header fixed-ribbon fixed-navigation">
        <header>Header</header>
        <aside id="left-panel">
            <ul>
                <li><a href="{{ path('php_orchestra_cms_nodeform') }}">Edition de noeud</a></li>
                <li><a href="{{ path('php_orchestra_cms_templateform') }}">Edition de template</a></li>
            </ul>
        </aside>
        <div id="main" role="main">
            <div id="ribbon"></div>
            <div id="content" style="opacity: 1;height:500px;">
                {% block content %}{% endblock %}
            </div>
        </div>
    </body>
</html>
