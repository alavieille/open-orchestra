Listing des types de contenus
<table border="1">
    <thead>
        <th>id</th><th>Identifiant</th><th>Nom</th><th>Version</th><th>Statut</th><th></th><th></th>
    </thead>
    <tbody>
    {% for contentType in contentTypes %}
    <tr>
        <td>{{ contentType.id }}</td>
        <td>{{ contentType.contentTypeId }}</td>
        <td>{{ contentType.name }}</td>
        <td>{{ contentType.version }}</td>
        <td>{{ contentType.status }}</td>
        <td><a href="{{ path('php_orchestra_cms_bo_contentType_edit', {'contentTypeId' : contentType.contentTypeId , 'id' : contentType.id}) }}">Editer</a></td>
        <td><input type="button" class="deleteContentType" data-contentType="{{ contentType.contentTypeId }}" value="Supprimer"></td>
    </tr>
    {% endfor %}
    </tbody>
</table>

{% include 'PHPOrchestraCMSBundle:BackOffice/Content:deleteContentTypeDialog.html.twig' %}
