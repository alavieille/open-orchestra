Listing des {{ contentType }}
<table border="1">
    <thead>
        <th>Id</th><th>Short Name</th><th>Version</th><th>Language</th><th>Status</th><th></th><th></th>
    </thead>
    <tbody>
    {% for content in contents %}
    <tr>
        <td>{{ content.contentId }}</td>
        <td>{{ content.shortName }}</td>
        <td>{{ content.version }}</td>
        <td>{{ content.language }}</td>
        <td>{{ content.status }}</td>
        <td><a href="">Editer</a></td>
        <td><a href="">Supprimer</a></td>
    </tr>
    {% endfor %}
    </tbody>
</table>